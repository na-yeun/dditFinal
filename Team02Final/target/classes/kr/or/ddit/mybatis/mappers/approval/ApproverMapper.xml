<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.approval.dao.ApproverMapper">

    <insert id="insertApprover" parameterType="ApproverVO">
        INSERT INTO APPROVER (
             APPROVER_NUM
            ,APPRLINE_ID
            ,EMP_ID
            ,APPROVER_FINAL_YN
        ) VALUES (
                      #{approverNum}
                     ,#{apprlineId}
                     ,#{empId}
                     ,#{approverFinalYn}
                 )
    </insert>

</mapper>
